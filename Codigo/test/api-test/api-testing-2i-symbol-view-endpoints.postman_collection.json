						"exec": [
							"const reqObject = {",
							"  url: 'https://inteligencia-industrial-2i.herokuapp.com/user',",
							"  method: 'GET'",
							"};",
							"",
							"pm.sendRequest(reqObject, (err, res) => {",
							"    var uid = res.json()[0].uid;",
							"    pm.collectionVariables.set(\"uid\", uid);",
							"});",
							"",
							"const reqObject2 = {",
							"  url: 'https://inteligencia-industrial-2i.herokuapp.com/symbol',",
							"  method: 'GET'",
							"};",
							"",
							"pm.sendRequest(reqObject2, (err, res) => {",
							"    var symbol = res.json()[0].id;",
							"    pm.collectionVariables.set(\"symbol_id\", symbol);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
